.title_1 {
  font-size: 35px;
  text-align: center;
}
.title_2 {
  font-size: 25px;
  text-align: center;
  margin: 10px;
}
image {
  width: 280px;
  height: 190px;
  margin-top: 200rpx;
}
.protocol {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 260rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 500rpx;
}
.pic {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 100rpx;
}
.protocol_text {
  font-size: 10px;
  font-weight: 400;
}
.wx-radio-input {
  width: 15rpx !important;
  height: 15rpx !important;
}
.radio {
  margin-top: -10rpx;
}
.button {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 120rpx;
  left: 50%;
  transform: translateX(-50%);
}
.button_left {
  display: flex;
  justify-content: center;
  align-items: center;
  border: #52D4A2 solid 1px;
  border-radius: 10rpx;
  color: #52D4A2;
  font-size: 40rpx;
  height: 112rpx;
  width: 350rpx;
  margin-top: 50rpx;
  margin-right: 20rpx;
}
.button_right {
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #52D4A2;
  border-radius: 10rpx;
  color: white;
  font-size: 40rpx;
  height: 112rpx;
  width: 350rpx;
  margin-top: 50rpx;
}